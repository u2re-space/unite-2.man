{
    "name": "unite-2",
    "description": "Web App Runtime Loader",
    "version": "1.0.0",
    "private": false,
    "type": "module",
    "scripts": {
        "test": "npm -ws run test --if-present --silent"
    },
    "browserslist": [
        "last 1 chrome version",
        "last 1 edge version"
    ],
    "workspaces": [
        "modules/projects/*",
        "apps/*"
    ],
    "overrides": {
        "@rollup/rollup-win32-x64-msvc": "npm:@rollup/wasm-node@latest",
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "rollup": "npm:@rollup/wasm-node@latest",
        "@vitejs/devtools": ">=0.0.0-alpha.32",
        "vite-plugin-pwa": ">=1.2.0"
    },
    "optionalDependencies": {
        "rollup": "npm:@rollup/wasm-node@latest",
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "@rollup/rollup-win32-x64-msvc": "npm:@rollup/wasm-node@latest",
        "@rollup/wasm-node": ">=4.57.1"
    },
    "resolutions": {
        "@rollup/rollup-win32-x64-msvc": "npm:@rollup/wasm-node@latest",
        "@rollup/rollup-linux-x64-gnu": "npm:@rollup/wasm-node@latest",
        "rollup": "npm:@rollup/wasm-node@latest"
    },
    "devDependencies": {
        "@rollup/wasm-node": ">=4.57.1",
        "rollup": "npm:@rollup/wasm-node@latest",
        "typescript": ">=6.0.0-beta",
        "eslint": "^10.0.0",
        "postcss-preset-env": "^11.1.3",
        "postcss-scss": "^4.0.9",
        "prettier": "^3.8.1",
        "stylelint": "^17.3.0",
        "stylelint-config-standard-scss": "^17.0.0",
        "stylelint-plugin-logical-css": "^2.0.2",
        "stylelint-scss": "^7.0.0"
    }
}
